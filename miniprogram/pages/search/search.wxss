/* pages/search/search.wxss */
@import "../test/test.wxss";

.input-search {
  background-color: rgb(228, 69, 29);
}

.youcan {
  font-family: 华文新魏;
  color: #312e21;
}

.block-youcan {
  text-align: center;
  width: 100%;
  /* background-color: rgb(247, 244, 242); */
  padding-bottom: 10rpx;
  padding-top: 10rpx;
  margin-top: 20rpx;
}

.tar {
  background: #cbcecc46;
  border: 2rpx solid #cbcecc46;
  margin-right: 20rpx;
  margin-bottom: 20rpx;
  border-radius: 25rpx;
  padding: 5rpx;
  font-size: 30rpx;
  color: rgba(0, 0, 0, 0.466);
}

.tags-row {
  margin: 10rpx;
}

page {
  line-height: 1.6;
  font-family: -apple-system-font, Helvetica Neue, sans-serif;
}

.search-bar {
  position: relative;
  padding: 8px 10px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  box-sizing: border-box;
  background-color: #92d597;
}

.search-bar__label {
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  z-index: 2;
  border-radius: 3px;
  text-align: center;
  color: #fff;
  background: #c1ebc5;
  line-height: 28px;
}

.search-bar__form {
  position: relative;
  -webkit-box-flex: 1;
  -webkit-flex: auto;
  flex: auto;
  border-radius: 5px;
  background: #c1ebc5;
}

.search-bar__box {
  position: relative;
  padding-left: 30px;
  padding-right: 30px;
  width: 100%;
  box-sizing: border-box;
  z-index: 1;
}

.icon-search_in-box {
  position: absolute;
  left: 10px;
  top: 7px;
}

.search-bar__input {
  height: 28px;
  line-height: 28px;
  font-size: 14px;
}

.icon-clear {
  position: absolute;
  top: 0;
  right: 0;
  padding: 7px 8px;
  font-size: 0;
}

.search-bar__cancel-btn {
  margin-left: 10px;
  line-height: 28px;
  color: #09bb07;
  white-space: nowrap;
}

.icon-search {
  margin-right: 8px;
  font-size: inherit;
}

.search-bar__text {
  display: inline-block;
  font-size: 14px;
  vertical-align: middle;
}

.cell {
  padding: 12px 15px 12px 35px;
}

.cells {
  position: relative;
  margin-top: 1.17647059em;
  background-color: #fff;
  line-height: 1.41176471;
  font-size: 17px;
}

.cells:before {
  top: 0;
  border-top: 1rpx solid #d9d9d9;
}

.cells:after, .cells:before {
  content: " ";
  position: absolute;
  left: 0;
  right: 0;
  height: 1px;
  color: #d9d9d9;
}

.cells:after {
  bottom: 0;
  border-bottom: 1rpx solid #d9d9d9;
}

.cell {
  padding: 10px 15px;
  position: relative;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
}

.cell:before {
  content: " ";
  position: absolute;
  top: 0;
  right: 0;
  height: 1px;
  border-top: 1rpx solid #d9d9d9;
  color: #d9d9d9;
  left: 15px;
}

.cell:first-child:before {
  display: none;
}

.searchbar-result {
  margin-top: 2rpx;
  font-size: 14px;
}

.searchbar-result:before {
  display: none;
}

.cell__bd {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
}

.cell_active {
  background-color: #9b9b9b;
}

icon {
  vertical-align: middle;
}
.tar{
  width: 250rpx;
height: 50rpx;
margin:40rpx;
float: left;
  text-align: center;
  line-height: 50rpx;
}
