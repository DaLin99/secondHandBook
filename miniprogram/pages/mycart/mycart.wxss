/*书的列表*/
.item-container{
  font-family: 华文新魏;
  display: flex;
  flex-direction: row;
  width: 100%;
  height: 300rpx;
  border-bottom: solid 1px rgb(165, 160, 160);
}

.selected-container{
  width: 10%;
  display: flex;
  align-items: center;
  justify-content: center;
}

.book-container{
  width: 70%;

  display: flex;
  flex-direction: row;
}
.book-img{
  width: 50%;
  height: 300rpx;

   display: flex;
  align-items: center;
  justify-content: center;
}
.book-img image{
 width: 110px;
 height: 130px;
  
}
.book-title{
  margin-top: 30rpx;
}
.book-inf{
  width: 50%;
   
    display: flex;
    flex-direction: column;
}
.trash-container{
  width: 20%;
  display: flex;
  align-items: center;
  justify-content: center;
}

.trash-container image{
  width: 50rpx;
  height: 50rpx;
  
}

.dividing-line{
  width: 100%;
  height: 1px;
  background-color: rgb(165, 160, 160);
}

.total-mon{
  font-family: 华文新魏;
  color: rgb(255, 123, 0);
}
.margin{
  width: 100%;
  height: 50px;
  margin-bottom: 50px;
}
.mybuttom{
 width: 100%;
 height: 50px;
 display: flex;
 flex-direction: row;
 position: fixed;
 left: 0;
 right: 0;
 bottom: 0;
 z-index: 99;
}
.left{
  width: 60%;
  height: 50px;
  display: flex;
  justify-content: space-around;
  border-top: solid 1px rgb(214, 210, 210);
}
.right{
  width: 40%;
  height: 50px;
  background-color: #96e99c;
  text-align: center;
  line-height: 50px;
  color: white;
  font-family: 华文新魏;
}
.icon{
  font-family: 华文新魏;
}













